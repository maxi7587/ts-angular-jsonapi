{
    "compilerOptions": {
        "removeComments": false,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "ES5",
        "experimentalDecorators": true,
        "sourceMap": true,
        "declaration": true,
        "noResolve": false,
        "moduleResolution":"node"
    },
    "filesGob": [
        "src/library/**/*.ts",
        "!typings/**",
        "!node_modules/**"
    ]
}
